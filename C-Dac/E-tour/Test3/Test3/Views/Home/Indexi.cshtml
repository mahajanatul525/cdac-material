@model Test3.Models.TourBusiness

@{
    ViewBag.Title = "Indexi";
}


    
   <!-- Carousel indicators -->
@Html.Partial("_LayoutLogout")
    <div id="myCarousel" class="carousel slide">
   <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
     <li data-target="#myCarousel" data-slide-to="3"></li>
      <li data-target="#myCarousel" data-slide-to="4"></li>
      <li data-target="#myCarousel" data-slide-to="5"></li>
   </ol>   
   <!-- Carousel items -->
   <div class="carousel-inner">
      <div class="item active">
         <img src="@Url.Content("~/Images/slide1.jpg")" alt="First slide">
      </div>
      <div class="item">
         <img src="@Url.Content("~/Images/slide2.jpg")" alt="Second slide">
      </div>
      <div class="item">
         <img src="@Url.Content("~/Images/slide3.jpg")" alt="Third slide">
      </div>
             <div class="item ">
         <img src="@Url.Content("~/Images/slide4.jpg")" alt="Fourth slide">
      </div>
      <div class="item">
         <img src="@Url.Content("~/Images/slide5.jpg")" alt="Fifth slide">
      </div>
      <div class="item">
         <img src="@Url.Content("~/Images/slide6.jpg")" alt="Sixth slide">
      </div>
   </div>
   <!-- Carousel nav -->
   <a class="carousel-control left" href="#myCarousel" 
      data-slide="prev">&lsaquo;</a>
   <a class="carousel-control right" href="#myCarousel" 
      data-slide="next">&rsaquo;</a>
</div> 

    <!--For Google Map-->
   <div id="map" style="width:100%;height:500px"></div>
            <script type="text/javascript" src="~/Scripts/Google.js"></script>

<script src="https://maps.googleapis.com/maps/api/js?callback=myMap"></script>
	